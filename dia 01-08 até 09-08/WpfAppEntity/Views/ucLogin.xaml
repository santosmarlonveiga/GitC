        <TextBox x:Name="tbxLogin" FontSize="32" FontFamily="Comic Sans MS" HorizontalAlignment="Left" Height="40" Margin="194,104,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="268"/>
